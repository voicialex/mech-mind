{
    "algorithm_name": "ExampleInference",
    "confidence_threshold": 0.7,
    "min_object_size": 50,
    "model_path": "models/example_model.onnx",
    "input_size": [640, 480],
    "preprocessing": {
        "normalize": true,
        "mean": [0.485, 0.456, 0.406],
        "std": [0.229, 0.224, 0.225]
    },
    "postprocessing": {
        "nms_threshold": 0.5,
        "max_detections": 100
    },
    "output": {
        "save_results": true,
        "result_path": "results/",
        "visualization": true
    }
}
